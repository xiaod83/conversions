{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/config.yml", "destination": "/admin/config.yml" }
  ],
  "headers": [
    {
      "source": "/admin/config.yml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/yaml"
        }
      ]
    }
  ]
}
